testfactsum{
decl
fact o1;
int testfun(int n);
enddecl
int testfun(int n){
decl
int sum;
enddecl
begin
self.o1=new(fact);
sum=0;
while(n!=0)do
sum=sum+self.o1.findfactorial(n);
n=n-1;
endwhile;
return sum;
end
}
}
